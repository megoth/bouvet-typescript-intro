{
  "locals": {
    "name": "Introduction to TypeScript",
    "owner": "Arne Hassel",
    "description": "An introduction to TypeScript, to be presented at Bouvet"
  },
  "plugins": [
    "./plugins/slides.coffee"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "slides": [
    "intro",
    "agenda",
    "typescript",
    "tools",
    "compatibility",
    "alternatives",
    "conclusion",
    "end"
  ]
}
